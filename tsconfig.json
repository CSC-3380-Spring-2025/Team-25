{
  "compilerOptions": {
    "types": [
      "node",
      "src/types"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "baseUrl": ".",
    "target": "ESNEXT",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/(routes)/dashboard/page.tsx/dash.js"
, "src/utils/userSchema.ts", "src/app/(routes)/dashboard/budgets/page.tsx", "src/app/_components/Header.tsx", "src/app/page.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
